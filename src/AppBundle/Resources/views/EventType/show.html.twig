{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EventType</h1>

    <table class="table table-condensed table-bordered table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ entity.icon }}</td>
            </tr>
            <tr>
                <th>Colorname</th>
                <td>{{ entity.colorName }}</td>
            </tr>
            <tr>
                <th>Isprimary</th>
                <td>{{ entity.isPrimary }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('eventtype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('eventtype_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
